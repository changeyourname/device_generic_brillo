/dev/socket/dbus           u:object_r:dbus_daemon_socket:s0
/system/bin/dbus-daemon    u:object_r:dbus_daemon_exec:s0
/dev/socket/connectivity   u:object_r:init:s0

# Brillo system service binaries are 'brillo_exec' objects for now.
# E.g.
#/system/bin/testservice    u:object_r:brillo_exec:s0
#
/system/bin/firewalld      u:object_r:brillo_exec:s0
/system/bin/weaved         u:object_r:brillo_exec:s0
/system/bin/webservd       u:object_r:brillo_exec:s0
/system/bin/wifi_connect   u:object_r:connectivity_exec:s0
/system/bin/connectivity   u:object_r:connectivity_exec:s0
/dev/wcnss_wlan            u:object_r:connectivity_device:s0
/sys/devices/soc.0/a000000.qcom,wcnss-wlan/serial_number  u:object_r:connectivity_sysfs_entry:s0

# Defining the contexts for metricsd.
/data/misc/metrics(/.*)?    u:object_r:metrics_data_file:s0
/system/bin/metrics_daemon  u:object_r:brillo_exec:s0
/system/bin/metrics_client  u:object_r:brillo_exec:s0

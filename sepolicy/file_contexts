/system/bin/dbus-send      u:object_r:dbus_daemon_exec:s0
/dev/socket/connectivity   u:object_r:connectivity_socket:s0

/system/bin/core2md              u:object_r:crash_reporter_exec:s0
/system/bin/crash_reporter       u:object_r:crash_reporter_exec:s0
/system/bin/crash_sender         u:object_r:crash_reporter_exec:s0
/system/bin/periodic_scheduler   u:object_r:crash_reporter_exec:s0
/data/misc/crash_reporter(/.*)?  u:object_r:crash_reporter_data_file:s0

# Some Brillo system service binaries are still 'brillo_exec' objects, but
# introducing more-specific domains to avoid granting unneeded permissions is
# preferred.
/system/bin/avahi-daemon   u:object_r:brillo_exec:s0
/system/bin/firewalld      u:object_r:brillo_exec:s0

/system/bin/webservd       u:object_r:webservd_exec:s0
/data/misc/webservd(/.*)?  u:object_r:webservd_data_file:s0

/system/bin/wifi_connect   u:object_r:connectivity_exec:s0
/system/bin/wifi_init      u:object_r:connectivity_exec:s0
/system/bin/connectivity   u:object_r:connectivity_exec:s0

/data/misc/metrics(/.*)?   u:object_r:metrics_data_file:s0
/system/bin/metrics_daemon u:object_r:brillo_exec:s0
/system/bin/metrics_client u:object_r:brillo_exec:s0

/data/misc/weaved(/.*)?    u:object_r:weaved_data_file:s0
/system/bin/weaved         u:object_r:brillo_exec:s0

/data/misc/tlsdated(/.*)?  u:object_r:tlsdated_data_file:s0
/system/bin/tlsdated       u:object_r:brillo_exec:s0
/system/bin/tlsdate-helper u:object_r:brillo_exec:s0
/system/bin/tlsdate        u:object_r:brillo_exec:s0

/system/bin/nativepowerman u:object_r:nativepowerman_exec:s0

